{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ðŸš€ Iniciar AllanStore (Dev)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/nuxt/bin/nuxt.mjs",
      "args": [
        "dev"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "presentation": {
        "group": "AllanStore",
        "order": 1
      }
    },
    {
      "name": "ðŸ”¨ Build AllanStore",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/nuxt/bin/nuxt.mjs",
      "args": [
        "build"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "production"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "presentation": {
        "group": "AllanStore",
        "order": 2
      }
    },
    {
      "name": "ðŸ“¦ Generate AllanStore (Static)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/nuxt/bin/nuxt.mjs",
      "args": [
        "generate"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "production"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "presentation": {
        "group": "AllanStore",
        "order": 3
      }
    },
  ],
}